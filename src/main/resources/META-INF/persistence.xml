		<class>de.hszg.stud.svtsar.forpro_backend.entity.Category</class>
		<class>de.hszg.stud.svtsar.forpro_backend.entity.ProductInStock</class>
		<class>de.hszg.stud.svtsar.forpro_backend.entity.ProductInStore</class>
		<class>de.hszg.stud.svtsar.forpro_backend.entity.Stock</class>
		<class>de.hszg.stud.svtsar.forpro_backend.entity.Store</class>
		<properties>
			<property name="javax.persistence.schema-generation.database.action"
				value="none" />
			<!-- >property name="eclipselink.ddl-generation" value="none" /> <property 
				name="eclipselink.ddl-generation.output-mode" value="database" / -->
		</properties>
	</persistence-unit>
